{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2022",
		"lib": ["ES2022"],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true,
		"esModuleInterop": true
		// "noUnusedParameters": true,  /* Report errors on unused parameters. */
	},
	"exclude": []
}
